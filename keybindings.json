// Geben Sie Ihre Tastenzuordnungen in dieser Datei ein, um die Standardwerte au√üer Kraft zu setzen.auto[]
[
    {
        "key": "ctrl+k ctrl+z",
        "command": "angular.goToComponentWithTemplateFile"
    },
    {
        "key": "ctrl+k ctrl+y",
        "command": "angular.goToTemplateForComponent"
    },
    {
        "key": "ctrl+k ctrl+g ctrl+s",
        "command": "workbench.action.tasks.runTask",
        "args": "npm run startx"
    },
    {
        "key": "ctrl+k ctrl+g ctrl+d",
        "command": "workbench.action.tasks.runTask",
        "args": "npm run toDeploy"
    },
    {
        "key": "ctrl+k ctrl+g ctrl+w",
        "command": "workbench.action.tasks.runTask",
        "args": "npm run toWebComp"
    }
]